{
  "name": "e-commerce-api",
  "version": "1.0.0",
  "main": "listen.js",
  "scripts": {
    "setup-dbs": "psql -f ./db/setup.sql",
    "test": "jest --verbose",
    "prepare": "husky",
    "seed": "node ./db/seeds/run-seed.js",
    "start": "node ./api/listen.js",
    "seed-prod": "NODE_ENV=production npm run seed"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "type": "module",
  "jest": {
    "setupFilesAfterEnv": [
      "jest-sorted"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-flow": "^7.25.9",
    "@babel/preset-react": "^7.25.9",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-sorted": "^1.0.15",
    "lint-staged": "^15.2.10",
    "pg-format": "^1.0.4",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "pg": "^8.13.1"
  }
}
